  "scripts": {
    "dev": "npm run build:server && node --expose-gc dist/server.js",
    "dev:next": "next dev --turbo",
    "build": "npm run build:server && next build",
    "build:server": "npx tsc server.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --skipLibCheck",
    "start": "NODE_ENV=production npm run build:server && node --expose-gc dist/server.js",
    "start:next": "next start",
    "vercel-build": "npm run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stripe": "jest tests/stripe",
    "test:stripe:e2e": "node scripts/test-stripe-flow.js",
    "stripe:check-users": "node scripts/check-user-subscriptions.js",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "generate-types": "npx supabase gen types typescript --project-id YOUR_PROJECT_ID --schema public > types_db.ts",
    "memory:report": "node scripts/memory-monitor.js --report",
    "cleanup:logs": "node scripts/cleanup-logs.js",
    "cleanup:server-logs": "node scripts/cleanup-server-logs.js",
    "cleanup:all-logs": "npm run cleanup:logs && npm run cleanup:server-logs",
    "remove:emojis": "node scripts/remove-emojis.js",
    "remove:all-emojis": "node scripts/remove-all-emojis.js",
    "memory:cleanup": "node scripts/memory-monitor.js --cleanup",
    "memory:monitor": "node scripts/memory-monitor.js --monitor",
    "memory:thresholds": "node scripts/memory-monitor.js --thresholds",
    "passwords:regenerate": "node scripts/regenerate-expired-passwords.js",
    "passwords:status": "node scripts/regenerate-expired-passwords.js --status"
